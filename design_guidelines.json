        },
        "secondary": {
          "DEFAULT": "#8B5CF6",
          "foreground": "#FFFFFF",
          "description": "Vivid Violet. Use for AI-related features and magic moments."
        },
        "accent": {
          "DEFAULT": "#F43F5E",
          "foreground": "#FFFFFF",
          "description": "Neon Rose. Use for high-priority alerts or 'Hot' items."
        },
        "background": {
          "body": "#F8FAFC",
          "sidebar": "#0F172A",
          "card": "#FFFFFF"
