        "glass_surface": "bg-white/70 backdrop-blur-xl border border-white/20 shadow-sm"
      }
    },
    "typography": {
      "font_family": {
        "headings": "Outfit, sans-serif",
        "body": "Inter, sans-serif",
        "mono": "JetBrains Mono, monospace"
      },
      "imports": [
        "https://fonts.googleapis.com/css2?family=Outfit:wght@300;400;500;600;700&family=Inter:wght@300;400;500;600&family=JetBrains+Mono:wght@400;500&display=swap"
      ],
      "rules": {
        "headings": "font-bold tracking-tight text-slate-900",
        "subheadings": "font-medium tracking-normal text-slate-500",
        "body": "font-normal text-slate-600 leading-relaxed",
        "nav": "font-medium text-sm tracking-wide"
      }
    },
    "spacing": {
      "container_padding": "p-6 md:p-8 lg:p-12",
      "section_gap": "gap-8 md:gap-12",
      "element_gap": "gap-4"
    },
    "borders": {
      "radius": {
        "default": "rounded-lg",
        "card": "rounded-xl",
        "button": "rounded-md",
        "input": "rounded-md"
      },
      "width": "border",
      "style": "border-slate-200"
    },
    "shadows": {
      "card": "shadow-[0_2px_8px_rgba(15,23,42,0.04)]",
      "hover": "shadow-[0_8px_24px_rgba(15,23,42,0.08)]",
      "dropdown": "shadow-xl border border-slate-100",
      "glow": "shadow-[0_0_16px_rgba(79,70,229,0.3)]"
    }
  },
  "components": {
    "buttons": {
      "primary": "bg-primary text-primary-foreground hover:bg-primary/90 h-10 px-6 font-medium transition-all duration-200 active:scale-95 shadow-sm",
      "secondary": "bg-white text-slate-700 border border-slate-200 hover:bg-slate-50 hover:border-slate-300 h-10 px-6 font-medium transition-all duration-200",
      "ghost": "text-slate-600 hover:bg-slate-100 hover:text-slate-900",
      "destructive": "bg-red-50 text-red-600 hover:bg-red-100 border border-red-100"
    },
    "cards": {
      "base": "bg-white border border-slate-100 shadow-[0_2px_12px_-4px_rgba(0,0,0,0.04)] rounded-xl overflow-hidden",
      "interactive": "hover:border-indigo-100 hover:shadow-[0_4px_20px_-4px_rgba(79,70,229,0.1)] transition-all duration-300 cursor-pointer"
    },
    "inputs": {
      "base": "bg-slate-50 border-slate-200 text-slate-900 focus:bg-white focus:ring-2 focus:ring-indigo-100 focus:border-indigo-500 transition-all duration-200 placeholder:text-slate-400"
    },
    "navigation": {
      "sidebar": "bg-slate-900 text-slate-300 w-64 h-screen fixed left-0 top-0 border-r border-slate-800",
      "sidebar_item": "flex items-center gap-3 px-4 py-3 rounded-md hover:bg-slate-800 hover:text-white transition-colors text-sm font-medium",
      "sidebar_item_active": "bg-indigo-600 text-white shadow-lg shadow-indigo-900/50"
    }
  },
  "layout_strategies": {
    "dashboard": {
      "grid": "grid grid-cols-1 md:grid-cols-12 gap-6",
